<div class="android-section-title mdl-typography--display-1-color-contrast mdl-cell mdl-cell--4-col mdl-cell--4-col-tablet mdl-cell--12-col-desktop" style="  width: 100%;"> Food Suggestions</div>


<% suggestionsList.forEach(function(item) { %>
<div class="demo-updates mdl-card mdl-shadow--2dp mdl-cell mdl-cell--4-col mdl-cell--4-col-tablet mdl-cell--12-col-desktop">
    <div style="background: url('breakfast.jpg'); background-repeat: no-repeat; background-size: 100%;" class="mdl-card__title mdl-card--expand mdl-color--teal-300">
        <h2 class="mdl-card__title-text"><%- item %></h2>
    </div>
    <div class="mdl-card__supporting-text mdl-color-text--grey-600">
        Non dolore elit adipisicing ea reprehenderit consectetur culpa.
    </div>
    <div class="mdl-card__actions mdl-card--border">
        <a id="clickme" href="http://maps.google.com/maps?q=<%- item %>" class="mdl-button mdl-js-button mdl-js-ripple-effect">Find restaurants near you!</a>
    </div>
</div>
<% }); %>